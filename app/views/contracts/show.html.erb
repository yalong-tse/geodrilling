<p id="notice"><%= notice %></p>
<div class="page-header">
  <h3>合同详情</h3>
</div>
<dl class="dl-horizontal">
  <dt><strong>合同名称:</strong></dt>
  <dd><%= @contract.name %></dd>
  <dt><strong>合同内容:</strong></dt>
  <dd><%= @contract.content %></dd>
  <dt><strong>签订日期:</strong></dt>
  <dd><%= @contract.signdate %></dd>
  <dt><strong>开始日期:</strong></dt>
  <dd><%= @contract.startdate %></dd>
  <dt><strong>结束日期:</strong></dt>
  <dd><%= @contract.finishdate %></dd>
  <dt><strong>合同状态:</strong></dt>
  <dd><%= @contract.status %></dd>
</dl>
<div class="form-actions">
  <%= link_to '编辑', edit_contract_path(@contract) ,:class=>"btn"%> |
  <%= link_to '增加钻孔', new_contract_hole_path(@contract) ,:class=>"btn"%> |
  <%= link_to '返回', contracts_path,:class=>"btn btn-primary" %>
</div>
<%= setActiveTab("_contract") %>
