<div class="page-header">
  <h3>设备管理/设备入库/钻机入库单查看</h3>
</div>
<div class="container">
  <div class="span9">
    <h4>钻机入库单详情</h4>
  <table class="table table-hover">
    <tr>
      <th>项目名称</th>
      <th>值</th>
    </tr>
    <tr>
      <td>钻机型号</td>
      <td><%= @rigmachine.rigmodel%></td>
    </tr>
    <tr>
      <td>钻孔直径</td>
      <td><%= @rigmachine.pipeDiameter %></td>
    </tr>
    <tr>
      <td>钻机功率</td>
      <td><%= @rigmachine.rigPower %></td>
    </tr>
    <tr>
      <td>钻孔深度</td>
      <td><%= @rigmachine.righoleDeep %></td>
    </tr>
    <tr>
      <td>钻机重量</td>
      <td><%= @rigmachine.rigweight %></td>
    </tr>
    <tr>
      <td>转速</td>
      <td><%= @rigmachine.rotatingSpeed %></td>
    </tr>
    <tr>
      <td>外观尺寸</td>
      <td><%= @rigmachine.overallsize %></td>
    </tr>
    <tr>
      <td>厂商</td>
      <td><%= @rigmachine.manufacture %></td>
    </tr>
    <tr>
      <td>厂商联系方式</td>
      <td><%= @rigmachine.manufactureContact %></td>
    </tr>
    <tr>
      <td>图片</td>
      <td>
        <ul class="thumbnails">
          <li class="span2">
            <div class="thumbnail">
              <img data-src="holder.js/260x180" alt="钻塔图片" src="/assets/zuanta.jpg"/>
            </div>
         </li>
       </ul>
     </td>
    </tr>
    <tr>
      <td>备注</td>
      <td><%= @rigmachine.remark %></td>
    </tr>
  </table>
</div>
</div>
